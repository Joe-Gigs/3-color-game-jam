pico-8 cartridge // http://www.pico-8.com
version 8
__lua__
function _init()
	--todo
	--inventory system :)
	player = {}
	--sprite in progress
	player.sp = 138
	player.x = 0
	player.y = 40
	player.speed = 0.6
	player.walk = false
	player.flip = false

	npc1 = {}
	npc1.sp = 154
	npc1.x = 40
	npc1.y = 60
	npc1.speed = 0.6
	npc1.flip = false
	npc1.sleep = false
	
	--stand in bad guy
	reapah = {}
	reapah.sp = 0
	reapah.x = 10
	reapah.y = 10

	cursor = {}
	cursor.sp = 8
	cursor.x = 0
	cursor.y = 40
	
	game_state = "explore"

	menu = {}
	menu.level = 0

	timer = 0

	talk = false
end

function _draw()
	print(player.x, 1, 40, 8)
	print(npc1.x, 1, 50, 11)
	print(npc1.sleep, 1, 30, 3)
	print(talk, 1,20, 10)
	
end

function _update()
	timer +=1
	if timer > 100 then
		resettimer()
	end

	playermovement()
	movecursor()
	menuinput()
	
	npcmovement()
	reversenpc()

--Testing encounter screen with hard coded values for player
	-- if player.x >= 24.6 and player.y <= 37.6 then
	-- 	triggerbattletest()
	-- end
	cls()

if btn(4) then
	if player.x then
		npcSpeak()
	end
end
	--todo put explore code in reusable function
	if game_state == "fight" then
		poke(0x5f2c, 3)
		drawmenu(reapah)
	else
		poke(0x5f2c, 0)
		map(0,0,0,0,16,16)
		spr(player.sp, player.x, player.y,1,1,player.flip)
		spr(npc1.sp, npc1.x, npc1.y, 1, 1, npc1.flip)
		menu.level = 0
	end
end
-----------------------------------------------
function playermovement()
	if game_state == "explore" then
		--left arrow key
		if btn(0) then
			player.x -= player.speed
			player.flip = true
			player.walk = true
			player.sp += 1

			if player.sp > 139 then
				player.sp = 138
			end
		end

		--right arror key
		if btn(1) then
			player.x += player.speed
			player.flip = false
			player.walk = true
			player.sp += 1

			if player.sp > 139 then
				player.sp = 138
			end
		end
		if btn(2) then
			player.y -= player.speed
			player.sp = 140
		end
		if btn(3) then
			player.y += player.speed
			player.sp = 142
		end
	end
end

function npcmovement()
	if talk == false and timer < 50 then
		npc1.x += 0.4
		npc1.flip = false
	end
end

function reversenpc()
	if timer > 50 and talk == false then
		npc1.x -= 0.4
		npc1.flip = true
	end
end

function npcSpeak()
	menu.sp = 64
	menu_right = {}
	menu_right.x = 30
	menu_right.y = 80

	menu_left = {}
	menu_left.x = 60
	menu_left.y = 80

	spr(menu.sp, menu_right.x, menu_right.y, 4, 4)
	spr(menu.sp, menu_left.x, menu_left.y, 4, 4)

	print("hello!", 33, 81, 0)
	talk = true

	npc1.sleep = true
end

function resettimer()
	timer = 0
 end

function triggerbattletest()
	game_state = "fight"
end

function drawmenu(entity) 

	menu.sp = 64

	menu_right = {}
	menu_right.x = 30
	menu_right.y = 40

	menu_left = {}
	menu_left.x = 0
	menu_left.y = 40

	action = false
	item = false

	menuposx = cursor.x
	menuposy = cursor.y

	spr(menu.sp, menu_right.x, menu_right.y, 4, 4)
	spr(menu.sp, menu_left.x, menu_left.y, 4, 4)

	spr(entity.sp, entity.x, entity.y, 4, 4)

	if menu.level == 0 then
		print("action", 5, 42, 0)
		print("item",40,42,0)
		print("leave", 40, 55, 0)
	end	
	if menu.level == 1 then
		print("menu1", 5, 42, 0)
	end
	if menu.level == 2 then
		print("menu2", 5, 42, 0)
	end
	if menu.level == 3 then
		game_state = "explore"
	end
	spr(cursor.sp,cursor.x,cursor.y)
end

function movecursor()
	if btnp(1) then
		cls()
		cursor.x=cursor.x + 35
	end
	if btnp(3) then
		cls()
		cursor.y=cursor.y + 13
	end
	if btnp(2) then
		cls()
		cursor.y=cursor.y - 13
	end
	if btnp(0) then
		cls()
		cursor.x=cursor.x - 35
	end
end

function menuinput()
if btnp(4) then
	if menuposx == 0 and menuposy == 40 then
		if menu.level == 0 then 
			cls() 
			menu.level = 1
			action = true
			end
		end 
	if menuposx == 35 and menuposy == 40 then
		cls()
		menu.level = 2
	end
	if menuposx == 35 and menuposy == 53 then
		cls()
		menu.level = 3
		game_state = "explore"
	
	end	
end
	if btnp(5) then
		menu.level = 0
	end
end





